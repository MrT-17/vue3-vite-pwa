{
  "name": "vue3-vite-pwa-starter-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm-run-all generate-assets --parallel type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/",
    "generate-base": "pwa-asset-generator public/pwa-logo.svg --index index.html --manifest public/manifest.webmanifest --portrait-only ",
    "generate:icons": "pnpm run generate-base public/icons --icon-only --path-override /icons --padding \"calc(50vh - 25%) calc(50vw - 50%)\"",
    "generate:splash": "pnpm run generate-base public/splash --splash-only --path-override /splash",
    "generate:format": "prettier --write index.html public/manifest.webmanifest",
    "generate-assets": "run-s generate:*"
  },
  "dependencies": {
    "pinia": "^3.0.4",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.3",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/eslint-plugin": "^1.5.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "~10.6.2",
    "jiti": "^2.6.1",
    "jsdom": "^27.3.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.7.4",
    "pwa-asset-generator": "^8.1.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.7",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.8",
    "workbox-build": "^7.4.0",
    "workbox-core": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0",
    "workbox-window": "^7.4.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  },
  "packageManager": "pnpm@10.25.0"
}
